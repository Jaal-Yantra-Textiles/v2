  "scripts": {
    "build": "medusa build && yarn run resolve:aliases",
    "seed": "medusa exec ./src/scripts/seed.ts",
    "start": "medusa start",
    "dev": "medusa develop",
    "test:integration:http": "TEST_TYPE=integration:http NODE_OPTIONS=\"--experimental-vm-modules --max-old-space-size=8192\" jest --silent=false --runInBand --forceExit",
    "test:integration:http:shared": "TEST_TYPE=integration:http NODE_OPTIONS=\"--experimental-vm-modules --max-old-space-size=8192\" jest --silent=false --runInBand --forceExit --config=./integration-tests/jest-shared.config.js",
    "test:integration:http:batched": "node ./scripts/run-batched-tests.js",
    "test:integration:modules": "TEST_TYPE=integration:modules NODE_OPTIONS=--experimental-vm-modules jest --silent --runInBand --forceExit",
    "test:unit": "TEST_TYPE=unit NODE_OPTIONS=--experimental-vm-modules jest --silent --runInBand --forceExit",
    "predeploy": "medusa db:migrate",
    "generate-ui": "medusa exec ./src/scripts/generate-ui-v2.ts",
    "resolve:aliases": "tsc --showConfig -p tsconfig.json > tsconfig.resolved.json && tsc-alias -p tsconfig.resolved.json && rimraf tsconfig.resolved.json",
    "build:analytics": "node ./src/scripts/build-analytics.js"
  },
